{
  "name": "i40-data-manager",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "test": "mocha -r ts-node/register --exit test/**/*.spec.ts",
    "coverage": "nyc --reporter html --reporter text npm test",
    "build": "tsc",
    "clean": "rm -rf ./dist/* ",
    "start": "node ./dist/server.js"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/SAP/i40-aas.git"
  },
  "keywords": [],
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "aas-logger": "link:../../pkg/aas-logger",
    "axios": "^0.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^5.0.0-alpha.7",
    "express-basic-auth": "^1.2.0",
    "i40-aas-objects": "0.2.6",
    "swagger-ui-express": "^4.0.6"
  },
  "devDependencies": {
    "sinon": "^7.5.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.8",
    "@types/sinon": "^7.0.13",
    "@types/swagger-ui-express": "^3.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.5.2"
  }
}
