export CORE_REGISTRIES_ENDPOINTS_DATABASE_HOST=localhost
export CORE_REGISTRIES_ENDPOINTS_DATABASE_PORT=5433
export CORE_REGISTRIES_ENDPOINTS_DATABASE_NAME=postgres
export CORE_REGISTRIES_ENDPOINTS_DATABASE_USER=postgres
export CORE_REGISTRIES_ENDPOINTS_DATABASE_PASSWORD=postgres
export CORE_REGISTRIES_ENDPOINTS_USER=admin
export CORE_REGISTRIES_ENDPOINTS_PASSWORD=admin
export CORE_REGISTRIES_ENDPOINTS_PORT=4400
export LOGGING_LOGLEVEL=DEBUG
export TLS_ENABLED=false
export TLS_KEYFILE=src/compose/volumes/certs/i40-aas.key.pem
export TLS_CERTFILE=src/compose/volumes/certs/i40-aas.crt.pem


docker run --rm --name test-postgres -e POSTGRES_PASSWORD=postgres -d -p 5433:5432 postgres:alpine
docker run -p 80:80 --name test-pgadmin -e 'PGADMIN_DEFAULT_EMAIL=user@domain.com'     -e 'PGADMIN_DEFAULT_PASSWORD=SuperSecret'     -d dpage/pgadmin4
