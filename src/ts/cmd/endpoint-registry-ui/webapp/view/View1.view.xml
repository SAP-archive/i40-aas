<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="i40-aas-registry-ui.i40-aas-registry-ui.controller.View1">
	<App id="idAppControl">
		<pages>
			<Page title="i40-AAS-Registry-UI">
				<Panel headerText="{i18n>descriptors}">
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>createNewDescriptor}" press="onCreateDescriptorPress">
						<TileContent>
							<ImageContent src="sap-icon://write-new"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>showAllDescriptors}" press="onShowAllDescriptorsPress">
						<TileContent>
							<ImageContent src="sap-icon://show"/>
						</TileContent>
					</GenericTile>
				</Panel>
				<Panel headerText="{i18n>semanticProtocols}">
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>createNewSP}" press="onCreateSemanticProtocolPress">
						<TileContent>
							<ImageContent src="sap-icon://write-new"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>showAllSemanticProtocols}"
						press="onShowAllSemanticProtocolsPress">
						<TileContent>
							<ImageContent src="sap-icon://show"/>
						</TileContent>
					</GenericTile>
				</Panel>
			</Page>
		</pages>
	</App>
</mvc:View>