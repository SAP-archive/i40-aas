 {
 	"welcomeFile": "/index.html",
 	"authenticationMethod": "none",
 	"logout": {
 		"logoutEndpoint": "/do/logout"
 	},
 	"routes": [{
 		"source": "^/AASDescriptors",
 		"target": "/AASDescriptors",
 		"destination": "AASServiceRegistry"
 	}, {
 		"source": "^/semanticProtocols",
 		"target": "/semanticProtocols",
 		"destination": "AASServiceRegistry"
 	}, {
 		"source": "^(.*)$",
 		"target": "$1",
 		"service": "html5-apps-repo-rt",
 		"authenticationType": "xsuaa"
 	}]
 }